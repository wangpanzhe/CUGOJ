#include<iostream>
#include<cmath>

#include<string>

using namespace std;



int main()

{

	long long a,b;

	string r;

 

	while(cin>>a>>b)

	{
		char s[100];

		if(a==0&&b==0)

		{

			break;

		}
		if(a,b>=0&&a,b<=pow(10,30))
		{
		
			int i;
			i=0;
			long long c=a+b;
			while (c)
			{
				int t=c%10;
				s[i]=t+'0';
				i++;
				c/=10;
			}
			for(int j=i-1;j>=0;j--)
			{
				r+=s[j];
			}
			r+="\n";
		}
	}
	cout<<r<<endl;
	return 0;

}
