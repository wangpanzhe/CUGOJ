#include<iostream>
using namespace std;
int main()
{
	int n,k,a[1000],sum=0;
	cin>>n>>k;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
		sum+=a[i];
	}
	int j=sum/k;
	if(j==0) cout<<"0"<<endl;
	else{
	
	for(;j>0;j--)
	{
		int num=0;
		for(int i=0;i<n;i++)
		{
			num+=a[i]/j;
		}
		if(num>=k)
		{
			cout<<j<<endl;
			break;
		}
	}
		}
	return 0;
}
